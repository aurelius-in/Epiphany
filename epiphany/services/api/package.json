{
  "name": "@epiphany/api",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "tsup src/index.ts --dts --format esm,cjs --splitting false",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prepare": "prisma generate --schema=../../ops/prisma/schema.prisma"
  },
  "prisma": {
    "schema": "../../ops/prisma/schema.prisma"
  },
  "dependencies": {
    "@bullmq/pro": "^1.0.0",
    "@prisma/client": "^5.17.0",
    "aws-sdk": "^2.1618.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "morgan": "^1.10.0",
    "zod": "^3.23.8",
    "bullmq": "^5.9.1",
    "ioredis": "^5.4.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "prisma": "^5.17.0",
    "tsup": "^8.0.2",
    "tsx": "^4.15.7",
    "typescript": "^5.5.3"
  }
}
